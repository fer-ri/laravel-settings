{
    "name": "ferri/laravel-settings",
    "description": "Laravel 5 persistent settings in database package",
    "keywords": [
        "laravel",
        "settings",
        "persistent",
        "database"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Ferri Sutanto",
            "email": "greenhouseprod@gmail.com"
        }
    ],
    "require": {
        "php" : ">=5.6.4",
        "illuminate/support": "5.1.*|5.2.*|5.3.*",
        "illuminate/database": "5.1.*|5.2.*|5.3.*",
        "illuminate/filesystem": "5.1.*|5.2.*|5.3.*"
    },
    "require-dev": {
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~5.0"
    },
    "autoload": {
        "psr-4": {
            "Ferri\\LaravelSettings\\": "src/"
        },
        "files": [
          "src/helpers.php"
        ]
      },
    "minimum-stability": "dev"
}
